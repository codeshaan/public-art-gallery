2xl\">\n          Upload an Art!\n        </h1>\n        {successMsg && (\n          <motion.div\n            className=\"bg-green-300 text-green-800 py-2 rounded-md px-4\"\n            initial={{ opacity: 0 }}\n            animate={{ opacity: 1 }}\n          >\n            {successMsg}\n          </motion.div>\n        )}\n        {error && (\n          <motion.div\n            className=\"bg-red-300 text-red-800 py-2 rounded-md px-4\"\n            initial={{ opacity: 0 }}\n            animate={{ opacity: 1 }}\n          >\n            {error}\n          </motion.div>\n        )}\n        <div className=\"h-1/2 my-4 w-full border-dashed border-4 border-gray-400 flex flex-col justify-center items-center hover:border-gray-700 cursor-pointer\">\n          <label htmlFor=\"file\">\n            <h3 className=\"text-gray-600 text-center font-bold text-xl mt-6\">\n              {file ? file.name : \"+ Select a file\"}\n            </h3>\n            <input\n              type=\"file\"\n              id=\"file\"\n              className=\"invisible\"\n              onChange={handleFile}\n            />\n          </label>\n        </div>\n        {progress && <ProgressBar progress={progress} />}\n      </motion.div>\n    </Modal>\n  );\n};\n\nexport default UploadModal;\n","/media/shaan/5C7CE0E67CE0BC40/projects-ubuntu/public-art-gallery/src/firebase/config.js",[],"/media/shaan/5C7CE0E67CE0BC40/projects-ubuntu/public-art-gallery/src/components/ProgressBar.js",[],"/media/shaan/5C7CE0E67CE0BC40/projects-ubuntu/public-art-gallery/src/Hooks/useStorage.js",[],"/media/shaan/5C7CE0E67CE0BC40/projects-ubuntu/public-art-gallery/src/components/Header.js",["116","117"],"import { useState } from \"react\";\n\nconst Header = () => {\n  const [search, setSearch] = useState(\"\");\n\n  return (\n    <header>\n      {/* <form action=\"\">\n        <input\n          type=\"text\"\n          placeholder=\"Search...\"\n          value={search}\n          className=\"form-controls\"\n        